{
  "dtisozt": {
    "prefix": "dtisozt",
    "body": [
      "${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}T${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}${CURRENT_TIMEZONE_OFFSET}"
    ],
    "description": "ISO date time stamp with timezone offset with a 'T' in the middle (${Y}-${m}-${d}T${H}:${M}:${S}${Z})"
  },
  "dtisozs": {
    "prefix": "dtisozs",
    "body": [
      "${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}${CURRENT_TIMEZONE_OFFSET}"
    ],
    "description": "ISO date time stamp with timezone offset with a space in the middle (${Y}-${m}-${d} ${H}:${M}:${S}${Z})"
  },
  "random16": {
    "prefix": "random16",
    "body": "${RANDOM_HEX}",
    "description": "6 random Base-16 digits"
  },
  "random10": {
    "prefix": "random10",
    "body": "${RANDOM}",
    "description": "6 random Base-10 digits"
  }
}
