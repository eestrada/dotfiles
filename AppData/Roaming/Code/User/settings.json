{
    "editor.insertSpaces": false,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80,
        120
    ],
    "editor.smoothScrolling": true,
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/.Trash": true,
        "**/CVS": true,
        "**/Thumbs.db": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.hg/store/**": true,
        "**/build/**": true,
        "**/node_modules/*/**": true
    },
    "git.autofetchPeriod": 600,
    "git.confirmSync": false,
    "gitlens.telemetry.enabled": false,
    "java.codeGeneration.generateComments": true,
    "java.codeGeneration.useBlocks": true,
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-1.8",
            "path": "/Library/Java/JavaVirtualMachines/amazon-corretto-8.jdk/Contents/Home"
        },
        {
            "name": "JavaSE-11",
            "path": "/Library/Java/JavaVirtualMachines/amazon-corretto-11.jdk/Contents/Home",
            "default": true
        },
        {
            "name": "JavaSE-17",
            "path": "/Library/Java/JavaVirtualMachines/amazon-corretto-17.jdk/Contents/Home"
        },
        {
            "name": "JavaSE-18",
            "path": "/Library/Java/JavaVirtualMachines/amazon-corretto-18.jdk/Contents/Home"
        }
    ],
    "sonarlint.disableTelemetry": true,
    "sort-json.settings.ignoreArraysOnDeepSort": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "telemetry.telemetryLevel": "off",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.defaultProfile.osx": "tmux-vscode",
    "terminal.integrated.profiles.osx": {
        "bash": {
            "path": "bash",
            "args": [
                "-l"
            ],
            "icon": "terminal-bash"
        },
        "fish": {
            "path": "fish",
            "args": [
                "-l"
            ]
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "tmux-vscode": {
            "path": "tmux",
            "icon": "terminal-tmux",
            "args": [
                "new-session",
                "-c",
                "${cwd}",
                "-t",
                "vscode",
                ";",
                "new-window",
                "-c",
                "${cwd}"
            ]
        },
        "zsh": {
            "path": "zsh",
            "args": [
                "-l"
            ]
        }
    },
    "terminal.integrated.profiles.windows": {
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Ubuntu-20.04 tmux (WSL)": {
            "path": [
                "${env:windir}\\Sysnative\\wsl.exe",
                "${env:windir}\\System32\\wsl.exe"
            ],
            "args": [
                "-d",
                "Ubuntu-20.04",
                "--cd",
                "${cwd}",
                "--",
                "tmux",
                "new-session",
                "-t",
                "vscode",
                "\\;",
                "new-window"
            ],
            "icon": "terminal-tmux"
        }
    },
    "workbench.colorTheme": "Monokai",
    "problems.sortOrder": "position",
    "todo-tree.general.periodicRefreshInterval": 5,
    "todo-tree.general.automaticGitRefreshInterval": 30,
    "todo-tree.filtering.ignoreGitSubmodules": true,
    "todo-tree.filtering.includeHiddenFiles": true,
    "todo-tree.tree.showCountsInTree": true,
    "aws.telemetry": false,
    "godot_tools.gdscript_lsp_server_port": 6005
}
